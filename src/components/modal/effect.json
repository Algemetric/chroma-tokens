{
  "effect": {
    "modal": {
      "transition": { "value": "{effect.transition.default}" },
      "backdrop": {
        "filter": { "value": "blur(0.2rem) saturate(50%)" }
      },
      "dialog": {
        "shadow": {
          "box": { "value": "{effect.shadow.box.xl}" }
        },
        "opened": {
          "transform": { "value": "scale({effect.scale.md}) translateY(0)" }
        },
        "closed": {
          "transform": {
            "value": "scale({effect.scale.sm}) translateY({size.spacing.md})"
          }
        }
      }
    }
  }
}
