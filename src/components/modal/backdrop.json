{
  "effect": {
    "backdrop": {
      "filter": { "value": "blur(0.2rem) saturate(50%)" },
      "transition": { "value": "0.05s all {effect.transition.type.fade-in}" }
    }
  },
  "color": {
    "backdrop": {
      "background": {
        "value": "rgba(0, 0, 0, {effect.opacity.intense})"
      }
    }
  }
}
