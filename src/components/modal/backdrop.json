{
  "effect": {
    "backdrop": {
      "filter": { "value": "blur(0.2rem) saturate(50%)" },
      "transition": { "value": "{effect.transition.default}" }
    }
  },
  "color": {
    "backdrop": {
      "background": {
        "value": "rgba({color.neutral.low.dark.rgb}, {effect.opacity.intense})"
      }
    }
  }
}
